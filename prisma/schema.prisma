generator client {
  provider = "prisma-client-js"
}

datasource postgresql {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  createdAt DateTime @default(now())
  email     String   @unique
  firstName String?
  id        Int      @default(autoincrement()) @id
  lastName  String?
  password  String?
  updatedAt DateTime @default(now())
}